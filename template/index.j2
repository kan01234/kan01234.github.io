<html>
	<head>
		<title>{{ data.name }} - {{ data.title }}</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		<link rel="stylesheet" href="assets/css/main.css" />
		<noscript><link rel="stylesheet" href="assets/css/noscript.css" /></noscript>
		<script src="https://kit.fontawesome.com/98f7a99f15.js" crossorigin="anonymous"></script>
	</head>
	<body class="is-preload">

		<!-- Wrapper -->
			<div id="wrapper">

				<!-- Header -->
					<header id="header" class="alt">
						<span class="logo icon solid fa-cat"></span>
						<h1>{{ data.name }}</h1>
						<p>{{ data.title }}</p>
					</header>

				<!-- Nav -->
					<nav id="nav">
						<ul>
							{% for item in data.navs %}
							<li>
								{% if item.url %}
								<a href="{{ item.url }}" {% if item.active %}class="active"{% endif %}>{{ item.title }}</a>
								{% else %}
								<a href="#{{ item.key }}" {% if item.active %}class="active"{% endif %}>{{ item.title }}</a>
								{% endif %}
							</li>
							{% endfor %}
						</ul>
					</nav>

				<!-- Main -->
					<div id="main">

						{% from 'intro-section.j2' import intro_section %}
						{{ intro_section(data.intro) }}

						{% from 'skill-section.j2' import skill_section %}
						{{ skill_section(data.skill) }}

						{% from 'exp-section.j2' import exp_section %}
						{{ exp_section(data.exp) }}

					</div>

				<!-- Footer -->
					<footer id="footer">
						<section>
							
						</section>
						<section>
							<h2>{{ data.contact.title }}</h2>
							<dl class="alt">
								<dt>Address</dt>
								<dd>{{ data.contact.address }}</dd>
								<dt>Email</dt>
								<dd><a href="mailto:{{ data.contact.email }}">{{ data.contact.email }}</a></dd>
							</dl>
							<ul class="icons">
								{% for other in data.contact.others %}
									<li><a href="{{ other.link }}" class="icon brands {{ other.icon }} alt"><span class="label">{{ other.text }}</span></a></li>
								{% endfor %}
							</ul>
						</section>
					</footer>

			</div>

		<!-- Scripts -->
			<script src="assets/js/jquery.min.js"></script>
			<script src="assets/js/jquery.scrollex.min.js"></script>
			<script src="assets/js/jquery.scrolly.min.js"></script>
			<script src="assets/js/browser.min.js"></script>
			<script src="assets/js/breakpoints.min.js"></script>
			<script src="assets/js/util.js"></script>
			<script src="assets/js/main.js"></script>

	</body>
</html>